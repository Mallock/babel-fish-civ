<GameData>
  <BaseGameText>
    <!-- AI accepts from human -->
    <Row Tag="LOC_DIPLO_ACCEPT_EMBASSY_FROM_HUMAN_ANY_ANY">
      <Text>हमें आपको हमारी राजधानी में एक दूतावास बनाने की अनुमति देने से खुशी हो रही है।</Text>
    </Row>
    <!-- AI rejects from human -->
    <Row Tag="LOC_DIPLO_REJECT_EMBASSY_FROM_HUMAN_ANY_ANY">
      <Text>हम इस समय इच्छुक नहीं हैं।</Text>
    </Row>
    <!-- AI requests from human -->
    <Row Tag="LOC_DIPLO_EMBASSY_FROM_AI_ANY_ANY">
      <Text>क्या आप हमें अपनी राजधानी में एक दूतावास स्थापित करने की अनुमति देंगे?</Text>
    </Row>
    <!-- Human accepts AI, AI responds -->
    <Row Tag="LOC_DIPLO_ACCEPT_EMBASSY_FROM_AI_ANY_ANY">
      <Text>खुशी का दिन!</Text>
    </Row>
    <!-- Human rejects AI, AI responds -->
    <Row Tag="LOC_DIPLO_REJECT_EMBASSY_FROM_AI_ANY_ANY">
      <Text>निराशाजनक।</Text>
    </Row>
    <!-- Choices -->
    <Row Tag="LOC_DIPLO_EMBASSY_CHOICE_NEGATIVE">
      <Text>हमारी राजधानी इस समय नए निवासियों को स्वीकार नहीं कर रही है।</Text>
    </Row>
    <Row Tag="LOC_DIPLO_EMBASSY_CHOICE_POSITIVE">
      <Text>आपके दूतावास हमारी राजधानी में स्वागत की अच्छी जोड़ी है।</Text>
    </Row>
  </BaseGameText>
</GameData>