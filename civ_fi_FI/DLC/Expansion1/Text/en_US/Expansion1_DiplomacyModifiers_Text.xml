<?xml version="1.0" encoding="utf-8"?>
<GameData>
  <EnglishText>
    <Row Tag="LOC_DIPLO_MODIFIER_DESCRIPTION_GOSSIP_POSITIVE">
      <Text>He rakastavat sitä, että jaat niin paljon tietoa</Text>
    </Row>
    <Row Tag="LOC_DIPLO_MODIFIER_DESCRIPTION_GOSSIP_NEGATIVE">
      <Text>He ovat onnettomia, että he eivät saa sinulta juoruja</Text>
    </Row>
    <Row Tag="LOC_DIPLO_MODIFIER_DESCRIPTION_SYCOPHANT_GOLDEN_AGE">
      <Text>They are happy to see you in a Golden Age</Text>
    </Row>
    <Row Tag="LOC_DIPLO_MODIFIER_DESCRIPTION_SYCOPHANT_DARK_AGE">
      <Text>He ovat pettäneitä, että olet pimeällä ajalla</Text>
    </Row>
    <Row Tag="LOC_DIPLO_MODIFIER_DESCRIPTION_SYMPATHIZER_DARK_AGE">
      <Text>He ovat tukenasi, kun selviydyt pimeästä ajasta</Text>
    </Row>
    <Row Tag="LOC_DIPLO_MODIFIER_DESCRIPTION_SYMPATHIZER_GOLDEN_AGE">
      <Text>He ovat kateellisia sinun Kultaisesta ajastasi</Text>
    </Row>
    <!-- Troops on border pledge modifiers -->
    <Row Tag="LOC_DIPLO_MODIFIER_BROKEN_PLEDGE">
      <Text>Rikkoimme lupauksemme siirtää joukkomme heidän rajoiltaan</Text>
    </Row>
    <Row Tag="LOC_DIPLO_MODIFIER_KEPT_PLEDGE">
      <Text>Pidimme lupauksemme siirtää joukkomme heidän rajoiltaan</Text>
    </Row>
    <Row Tag="LOC_DIPLO_MODIFIER_NO_PLEDGE">
      <Text>Päätimme olla lupaamatta siirtää joukkojamme heidän rajoiltaan</Text>
    </Row>
    <Row Tag="LOC_DIPLO_MODIFIER_SHARED_EMERGENCY">
      <Text>Liityimme hätätilanteeseen samaa henkilöä vastaan</Text>
    </Row>
  </EnglishText>
</GameData>