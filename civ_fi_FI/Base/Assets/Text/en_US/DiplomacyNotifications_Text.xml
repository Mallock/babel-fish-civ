<?xml version="1.0" encoding="utf-8"?>
<GameData>
  <BaseGameText>
    <!-- Default -->
    <Row Tag="LOC_DIPLOMACY_DEFAULT_NOTIFICATION_MESSAGE_INITIAL">
      <Text>Sinulla on viesti</Text>
    </Row>
    <Row Tag="LOC_DIPLOMACY_DEFAULT_NOTIFICATION_SUMMARY_INITIAL">
      <Text>{1_name} on lähettänyt sinulle viestin.</Text>
    </Row>
    <!-- Declare Friend -->
