<GameData>
  <BaseGameText>
    <!-- AI accepts from human -->
    <Row Tag="LOC_DIPLO_ACCEPT_EMBASSY_FROM_HUMAN_ANY_ANY">
      <Text>Olemme iloisia antaessamme sinulle luvan rakentaa suurlähetystön pääkaupunkiimme.</Text>
    </Row>
    <!-- AI rejects from human -->
    <Row Tag="LOC_DIPLO_REJECT_EMBASSY_FROM_HUMAN_ANY_ANY">
      <Text>Emme ole kiinnostuneita tällä hetkellä.</Text>
    </Row>
    <!-- AI requests from human -->
    <Row Tag="LOC_DIPLO_EMBASSY_FROM_AI_ANY_ANY">
      <Text>Sallitteko meidän perustaa suurlähetystön pääkaupunkiinne?</Text>
    </Row>
    <!-- Human accepts AI, AI responds -->
    <Row Tag="LOC_DIPLO_ACCEPT_EMBASSY_FROM_AI_ANY_ANY">
      <Text>Oi onnen päivää!</Text>
    </Row>
    <!-- Human rejects AI, AI responds -->
    <Row Tag="LOC_DIPLO_REJECT_EMBASSY_FROM_AI_ANY_ANY">
      <Text>Pettyvää.</Text>
    </Row>
    <!-- Choices -->
    <Row Tag="LOC_DIPLO_EMBASSY_CHOICE_NEGATIVE">
      <Text>Pääkaupunkimme ei tällä hetkellä ota vastaan uusia asukkaita.</Text>
    </Row>
    <Row Tag="LOC_DIPLO_EMBASSY_CHOICE_POSITIVE">
      <Text>Teidän suurlähetystönne on tervetullut lisäys pääkaupunkiimme.</Text>
    </Row>
  </BaseGameText>
</GameData>