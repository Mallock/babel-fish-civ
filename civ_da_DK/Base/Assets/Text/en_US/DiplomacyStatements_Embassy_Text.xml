<GameData>
  <BaseGameText>
    <!-- AI accepts from human -->
    <Row Tag="LOC_DIPLO_ACCEPT_EMBASSY_FROM_HUMAN_ANY_ANY">
      <Text>Vi er glade for at tillade dig at bygge en ambassade i vores hovedstad.</Text>
    </Row>
    <!-- AI rejects from human -->
    <Row Tag="LOC_DIPLO_REJECT_EMBASSY_FROM_HUMAN_ANY_ANY">
      <Text>Vi er ikke interesserede på dette tidspunkt.</Text>
    </Row>
    <!-- AI requests from human -->
    <Row Tag="LOC_DIPLO_EMBASSY_FROM_AI_ANY_ANY">
      <Text>Vil du tillade os at etablere en ambassade i din hovedstad?</Text>
    </Row>
    <!-- Human accepts AI, AI responds -->
    <Row Tag="LOC_DIPLO_ACCEPT_EMBASSY_FROM_AI_ANY_ANY">
      <Text>Åh lykkelige dag!</Text>
    </Row>
    <!-- Human rejects AI, AI responds -->
    <Row Tag="LOC_DIPLO_REJECT_EMBASSY_FROM_AI_ANY_ANY">
      <Text>Skuffende.</Text>
    </Row>
    <!-- Choices -->
    <Row Tag="LOC_DIPLO_EMBASSY_CHOICE_NEGATIVE">
      <Text>Vores hovedstad tager i øjeblikket ikke imod nye beboere.</Text>
    </Row>
    <Row Tag="LOC_DIPLO_EMBASSY_CHOICE_POSITIVE">
      <Text>Dit ambassade er en velkommen tilføjelse til vores hovedstad.</Text>
    </Row>
  </BaseGameText>
</GameData>